<?xml version='1.0' encoding='utf-8'?>
<widget id="wallet.dentacoin.com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="ios-package" value="CDVBarcodeScanner" />
    </feature>
    <feature name="FilePicker">
        <param name="ios-package" value="FilePicker" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="SocialSharing">
        <param name="ios-package" value="SocialSharing" />
        <param name="onload" value="true" />
    </feature>
    <name>Dentacoin Wallet</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="app-resources/images/wallet-app-icon-180px.png" width="180" />
    <icon height="60" src="app-resources/images/wallet-app-icon-60px.png" width="60" />
    <icon height="120" src="app-resources/images/wallet-app-icon-120px.png" width="120" />
    <icon height="76" src="app-resources/images/wallet-app-icon-76px.png" width="76" />
    <icon height="152" src="app-resources/images/wallet-app-icon-152px.png" width="152" />
    <icon height="40" src="app-resources/images/wallet-app-icon-40px.png" width="40" />
    <icon height="80" src="app-resources/images/wallet-app-icon-80px.png" width="80" />
    <icon height="57" src="app-resources/images/wallet-app-icon-57px.png" width="57" />
    <icon height="114" src="app-resources/images/wallet-app-icon-114px.png" width="114" />
    <icon height="72" src="app-resources/images/wallet-app-icon-72px.png" width="72" />
    <icon height="144" src="app-resources/images/wallet-app-icon-144px.png" width="144" />
    <icon height="29" src="app-resources/images/wallet-app-icon-29px.png" width="29" />
    <icon height="58" src="app-resources/images/wallet-app-icon-58px.png" width="58" />
    <icon height="50" src="app-resources/images/wallet-app-icon-50px.png" width="50" />
    <icon height="100" src="app-resources/images/wallet-app-icon-100px.png" width="100" />
    <icon height="167" src="app-resources/images/wallet-app-icon-167px.png" width="167" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="orientation" value="portrait" />
</widget>
