{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wallet-v2": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/assets"
            ],
            "styles": [
              "./src/assets/libs/bootstrap/css/bootstrap.min.css",
              "./src/assets/libs/combobox/combobox.css",
              "./src/assets/libs/font-awesome/css/font-awesome.min.css",
              "./src/assets/css/style.css"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./src/assets/libs/bootstrap/js/bootstrap.min.js",
              "./node_modules/bootbox.js/bootbox.min.js",
              "./src/assets/libs/combobox/combobox.js",
              "./node_modules/clipboard/dist/clipboard.min.js",
              "./node_modules/bignumber.js/bignumber.min.js",
              "./node_modules/decimal.js/decimal.min.js",
              "./src/assets/libs/truffle-contract/truffle-contract-compiled.js",
              "./src/assets/libs/qrcode/qrcode.js",
              "./src/assets/js/basic.js",
              "./src/assets/js/index-bundled.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "hybrid": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.hybrid.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wallet-v2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "wallet-v2:build:production"
            },
            "hybrid": {
              "browserTarget": "wallet-v2:build:hybrid"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wallet-v2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "wallet-v2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wallet-v2:serve:production"
            },
            "hybrid": {
              "browserTarget": "wallet-v2:build:hybrid"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/server",
            "main": "src/main.server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": {
                "scripts": false,
                "styles": true
              }
            }
          }
        }
      }
    }},
  "defaultProject": "wallet-v2"
}